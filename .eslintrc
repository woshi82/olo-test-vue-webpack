{
  "extends": "airbnb-base/legacy",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    // Disable for __DEV__, __SERVER__ usage.
    "no-undef" : 0,
    // Disable for console/alert
    "no-console": 0,
    "no-alert": 0,

    "indent": ["error", 4]
  },
  "globals": {
    "__DEV__": true
  }
}
